# Ignore data directory but keep config and gitkeep placeholders
data/*
!data/config.json
!data/*/
!data/**/.gitkeep

# Ignore temporary files
*.tmp
*~
__pycache__/
*.pyc

# Ignore binary assets and spreadsheets
assets/*
!assets/*.png
!assets/fonts/
!assets/fonts/*.ttf
!assets/icons/
!assets/icons/**/*.svg
*.xlsx
